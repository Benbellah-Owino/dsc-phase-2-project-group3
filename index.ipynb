   "source": [
    "best_movies=\"\"\"SELECT\n",
    "            budgets.movie,\n",
    "            bom_gross.studio,\n",
    "            tmdb_ratings.vote_average AS ratings,\n",
    "            ROUND((budgets.worldwide_gross-budgets.production_budget) / budgets.worldwide_gross,2) AS ROI,\n",
    "            budgets.worldwide_gross\n",
    "            FROM budgets\n",
    "            JOIN bom_gross ON\n",
    "            budgets.movie=bom_gross.title\n",
    "            JOIN tmdb_ratings ON\n",
    "            budgets.movie=tmdb_ratings.title\n",
    "            WHERE budgetS.production_budget > 0\n",
    "            ORDER BY ROI ASC,ratings DESC\n",
    "            LIMIT 10;\n",
    "            \"\"\"\n",
    "best_movies_df = pd.read_sql(best_movies, conn)\n",
    "best_movies_df\n",
    "\n"
   ]
