    "genre_ratings = \"\"\"\n",
    "SELECT\n",
    "    movies.genre AS genre,\n",
    "    ROUND(AVG(tmdb_ratings.vote_average), 2) AS avg_rating,\n",
    "    COUNT(*) AS num_movies\n",
    "FROM tmdb_ratings\n",
    "JOIN budgets \n",
    "    ON tmdb_ratings.title = budgets.movie\n",
    "JOIN movies\n",
    "    ON movies.studio = budgets.movie OR movies.genre IS NOT NULL\n",
    "GROUP BY movies.genre\n",
    "HAVING num_movies > 5\n",
    "ORDER BY avg_rating DESC\n",
    "LIMIT 10;\n",
    "\"\"\"\n",
    "\n",
    "genre_ratings_df = pd.read_sql(genre_ratings, conn)\n",
    "genre_ratings_df"
