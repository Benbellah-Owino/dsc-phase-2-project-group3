    "studio_performance = \"\"\"\n",
    "SELECT\n",
    "    bom_gross.studio,\n",
    "    ROUND(AVG(budgets.worldwide_gross), 2) AS avg_worldwide_gross,\n",
    "    COUNT(budgets.movie) AS num_movies\n",
    "FROM budgets \n",
