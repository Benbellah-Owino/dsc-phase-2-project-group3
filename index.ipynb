   "source": [
    "roi = \"\"\"\n",
    "SELECT\n",
    "    budgets.movie,\n",
    "    bom_gross.studio,\n",
    "    ROUND((budgets.worldwide_gross - budgets.production_budget) / budgets.production_budget, 2) AS ROI,\n",
    "    budgets.worldwide_gross,\n",
    "    budgets.production_budget\n",
    "FROM budgets\n",
    "JOIN bom_gross \n",
    "    ON budgets.movie = bom_gross.title\n",
    "WHERE budgets.production_budget > 0\n",
    "ORDER BY ROI DESC\n",
    "LIMIT 10;\n",
    "\"\"\"\n",
    "roi_df= pd.read_sql(roi,conn)\n",
    "roi_df"
   ]
